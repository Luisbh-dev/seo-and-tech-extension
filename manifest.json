{
  "manifest_version": 3,
  "name": "SEO & Tech Analyzer",
  "version": "1.6",
  "description": "Analyze SEO and technology stack of websites.",
  "icons": {
    "128": "icons/seo_tech_analyzer_icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "js/background.js"
  }
}
