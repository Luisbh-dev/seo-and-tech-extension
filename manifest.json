{
  "manifest_version": 3,
  "name": "SEO & Tech Analyzer",
  "version": "1.1",
  "description": "Analyze SEO and technologies used on a website.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/seo_tech_analyzer_icon.png",
      "128": "icons/seo_tech_analyzer_icon.png"
    }
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/seo_tech_analyzer_icon.png",
    "128": "icons/seo_tech_analyzer_icon.png"
  }
}
